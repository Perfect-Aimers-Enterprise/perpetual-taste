{
  "version": 2,
  "builds": [
    {
      "src": "/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [

    {
      "src": "/image/(.*)",
      "dest": "/public/image/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "functions": {
    "api/*": {
      "maxDuration": 60
    }
  }
}